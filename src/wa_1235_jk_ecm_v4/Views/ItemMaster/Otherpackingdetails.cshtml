	function validateCustomerBrand() {
		 var customer = $('#lblCustomerName').text().trim();
		 var brand = $('#lblBrandName').text().trim();

		 if (customer === "") {
			 alert("Customer is mandatory");
			 return false;
		 }

		 if (brand === "") {
			 alert("Brand is mandatory");
			 return false;
		 }

		 return true;
	 }  
	function OnchangeCellTape() {
		var CelloTapeId = $("#dllCelloTape").val();

		var json = {
			CelloTapeId: CelloTapeId
		};

		var JsonData = JSON.stringify(json);

		$.ajax({
			type: 'POST',
			url: '@Url.Action("GetSelectedCelloTape", "ItemMaster")',
			data: { JsonData: JsonData },
			datatype: 'json',
			success: function (data) {
				$("#cellotapepopup").modal("show");
				OpenDocument(data.celloTapeImage[0].ImageFileName);
			}
		});
	}

		async function OpenDocument(FileName) {	
		const path = 'others\\cellotape\\';

		 const blobpath = (normalizePathPrefix(blobPathPrefix) + normalizePathPrefix(path)).toLowerCase()+ FileName;

		const JSONData = {
			blobPath: blobpath,
			asSas: false,
			downloadFileName: FileName
		};

		try {
			$('#loader').show();

			const response = await $.ajax({
				url: blobStorageUrl + 'Files/get',
				headers: { Authorization: 'Bearer ' + JwToken },
				type: "POST",
				contentType: "application/json",
				data: JSON.stringify(JSONData),
				xhrFields: { responseType: 'blob' }
			});

			if (response && response.size > 0) {
				const objectURL = URL.createObjectURL(response);

				// Clear previous image
				const imageContainer = document.getElementById('imageContainer');
				imageContainer.innerHTML = "";

				// Create new image
				const imgElement = document.createElement("img");
				imgElement.src = objectURL;
				imgElement.alt = FileName;
				imgElement.style.maxWidth = "100%";
				imgElement.style.borderRadius = "8px";
				imgElement.style.boxShadow = "0 0 6px rgba(0,0,0,0.2)";

				imageContainer.appendChild(imgElement);

				// Optional: cleanup URL after 5s
				setTimeout(() => URL.revokeObjectURL(objectURL), 5000);
			} else {
				alert("⚠️ File not found or empty.");
			}
		} catch (error) {
			console.error("❌ Error fetching document:", error);			
		} finally {
			$('#loader').hide();
		}
	}

