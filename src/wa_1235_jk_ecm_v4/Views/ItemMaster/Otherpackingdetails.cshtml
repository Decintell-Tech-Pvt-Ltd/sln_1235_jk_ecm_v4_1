	function AddOtherPackingObjData(ActionFlag) {
		 if (!validateCustomerBrand()) {
			return;
		   }
		var TangTemperingId = $("#dllTangTempering").val();
		var TangColorId = $("#dllTangColor").val();
		var BlackodisingId = $("#dllBlackodising").val();
		var QualitySealId = $("#dllQualitySeal").val();
		var LineNoId = $("#dllLineNo").val();
		var HologramId = $("#dllHologram").val();
		var PriceStickerId = $("#dllPriceSticker").val();
		var MadeInIndiaId = $("#dllMadeInIndia").val();
		var PolytheneBagId = $("#dllPolytheneBag").val();
		var SilicaGelId = $("#dllSilicaGel").val();
		var FumigationId = $("#dllFumigation").val();
		var PromotionalItemId = $("#dllPromotionalItem").val();
		var StrapId = $("#dllStrap").val();
		var CelloTapeId = $("#dllCelloTape").val();
		var ShrinkWrapId = $("#dllShrinkWrap").val();
		var PaperwoolId = $("#dllPaperwool").val();
		var PONOId = $("#dllPONO").val();
		var PreInspection = $("#dllPreInspection").val();
		var StencilId = $("#dllStencil").val();
		var InsertQualityCardId = $("#dllInsertQualityCard").val();
		var BarCodeWrapper1Id = $("#dllBarCodeWrapper1").val();
		var BarCodeWrapper2Id = $("#dllBarCodeWrapper2").val();
		var BarCodeInnerId = $("#dllBarCodeInner").val();
		var BarCodeOuterId = $("#dllBarCodeOuter").val();
		var SapDescription = $("#txtSapDescription").val();
		var Remark = $("#txtRemark").val();

		var OtherPackingobjarr = {};


		OtherPackingobjarr.TangTempering = TangTemperingId;
		OtherPackingobjarr.TangColor = TangColorId;
		OtherPackingobjarr.Blackodising = BlackodisingId;
		OtherPackingobjarr.QualitySeal = QualitySealId;
		OtherPackingobjarr.LineNo = LineNoId;
		OtherPackingobjarr.Hologram = HologramId;
		OtherPackingobjarr.PriceSticker = PriceStickerId;
		OtherPackingobjarr.MadeInIndia = MadeInIndiaId;
		OtherPackingobjarr.PolytheneBag = PolytheneBagId;
		OtherPackingobjarr.SilicaGel = SilicaGelId;
		OtherPackingobjarr.Fumigation = FumigationId;
		OtherPackingobjarr.PromotionalItem = PromotionalItemId;
		OtherPackingobjarr.Strap = StrapId;
		OtherPackingobjarr.CelloTape = CelloTapeId;
		OtherPackingobjarr.ShrinkWrap = ShrinkWrapId;
		OtherPackingobjarr.Paperwool = PaperwoolId;
		OtherPackingobjarr.PONO = PONOId;
		OtherPackingobjarr.PreInspection = PreInspection;
		OtherPackingobjarr.Stencil = StencilId;
		OtherPackingobjarr.InsertQualityCard = InsertQualityCardId;
		OtherPackingobjarr.BarCodeWrapper1 = BarCodeWrapper1Id;
		OtherPackingobjarr.BarCodeWrapper2 = BarCodeWrapper2Id;
		OtherPackingobjarr.BarCodeInner = BarCodeInnerId;
		OtherPackingobjarr.BarCodeOuter = BarCodeOuterId;
		OtherPackingobjarr.SapDescription = SapDescription;
		OtherPackingobjarr.Remark = Remark;


		var Reqno = @ViewBag.NewReqNo; // Corrected variable name

		var mode = '@Context.Request.Query["Mode"]';

		var iOperationalFlag = 1;

		if (mode != '' && mode != 'undefined' && mode == 'Edit')
			iOperationalFlag = 2;
		else
			iOperationalFlag = 1;

		var iLoggedInUserID = parseInt('@Context.Session.GetInt32("CreatedBy")');

		var json = {
			Reqno: Reqno,
			iOperationalFlag: iOperationalFlag,
			ActionFlag: ActionFlag,
			iLoggedInUserID: iLoggedInUserID,
			Details: OtherPackingobjarr
		};

		var JSONInsert = JSON.stringify(json);

		$.ajax({
			type: 'POST',
			url: '@Url.Action("SaveOtherPackingDetails", "ItemMaster")',
			data: { JsonData: JSONInsert },
			datatype: 'json',
			success: function (data) {
				// alert(data.response);
				alert("Other packing details " + ActionFlag + " successfully");

				var pageID = encodeURIComponent('@Context.Request.Query["PageID"]');
				var parentID = encodeURIComponent('@Context.Request.Query["ParentID"]');
				var moduleID = encodeURIComponent('@Context.Request.Query["ModuleID"]');
				var modulePageID = encodeURIComponent('@Context.Request.Query["ModulePageID"]');
				var mode = '@Context.Request.Query["Mode"]';
				var finalMode = mode ? encodeURIComponent(mode) : 'Edit';
				var Display = '@Context.Request.Query["Display"]';
				var Status = encodeURIComponent('@Context.Request.Query["Status"]');
				//var ReqType = encodeURIComponent('@Context.Request.Query["ReqType"]');
				 var redirectUrl;
				if (Display == 'Approval') {
					   redirectUrl = '@Url.Action("ShowLabelLayout", "ItemMaster")' +
						'?PageID=' + pageID +
						'&ParentID=' + parentID +
						'&ModuleID=' + moduleID +
						'&ModulePageID=' + modulePageID +
						'&ReqValue=' + Reqno +
						'&Mode=Edit'+
						'&Display=Approval&Status=' + Status;
				}
				else if (Display == 'LabelLayout') {
					   redirectUrl = '@Url.Action("ShowLabelLayout", "ItemMaster")' +
						'?PageID=' + pageID +
						'&ParentID=' + parentID +
						'&ModuleID=' + moduleID +
						'&ModulePageID=' + modulePageID +
						'&ReqValue=' + Reqno +
						'&Mode=Edit'+
						'&Display=LabelLayout&Status=' + Status;
				}
				else if (finalMode == 'Edit') {
					redirectUrl = '@Url.Action("ShowLabelLayout", "ItemMaster")' +
						'?PageID=' + pageID +
						'&ParentID=' + parentID +
						'&ModuleID=' + moduleID +
						'&ModulePageID=' + modulePageID +
						'&ReqValue=' + Reqno +
						'&Mode=' + finalMode+'&Status=' + Status;
				}			
				else {
					var redirectUrl = '@Url.Action("ShowLabelLayout", "ItemMaster")' +
						'?PageID=' + pageID +
						'&ParentID=' + parentID +
						'&ModuleID=' + moduleID +
						'&ModulePageID=' + modulePageID +
						'&ReqValue=' + Reqno +
						'&Mode=' + finalMode+'&Status=' + Status;
				}

				window.location.href = redirectUrl;

			},
			error: function () {
				alert('Error occurred while processing your request.');
			}
		});
	}


