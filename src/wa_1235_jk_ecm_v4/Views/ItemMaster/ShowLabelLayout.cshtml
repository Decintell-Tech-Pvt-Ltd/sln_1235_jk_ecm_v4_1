    $(document).ready(function () {
        var isExpanded = false; // Start collapsed (i.e., cards are minimized)

        $('#toggleAllBtn').click(function () {
            $('.card').each(function () {
                var $card = $(this);
                var $body = $card.find('.card-body');
                var $collapseBtn = $card.find('[data-card-widget="collapse"]');

                // Check if this card is already collapsed
                var isVisible = $body.is(':visible');

                // Expand or collapse only if needed
                if (!isExpanded && !isVisible) {
                    $collapseBtn.trigger('click'); // Expand
                } else if (isExpanded && isVisible) {
                    $collapseBtn.trigger('click'); // Collapse
                }
            });

            // Toggle button label & title
            if (!isExpanded) {
                $(this).text('Collapse All').attr('title', 'Collapse All Grids');
            } else {
                $(this).text('Expand All').attr('title', 'Expand All Grids');
            }

            isExpanded = !isExpanded; // Flip state
        });
    });
</script>



